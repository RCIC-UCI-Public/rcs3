{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "retrieveETag",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::*xxxinventoryxxx/*"
            ]
        },
        {
            "Sid": "writeLogEvents",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:xxxregionxxx:xxxaccountidxxx:log-group:/aws/lambda/createS3BatchInput:*"
            ]
        }
    ]
}
