{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Deny",
            "Action": "s3:DeleteBucket",
            "Resource": [
                "arn:aws:s3:::ppapadop-tmpstore1-uci-bkup-bucket"
            ]
        },
        {
            "Effect": "Allow",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::ppapadop-tmpstore1-uci-bkup-bucket",
                "arn:aws:s3:::ppapadop-tmpstore1-uci-bkup-bucket/*"
            ],
            "Condition": {}
        },
        {
            "Effect": "Allow",
            "Action": "s3:ListAllMyBuckets",
            "Resource": "*",
            "Condition": {}
        },
        {
            "Effect": "Deny",
            "Action": "s3:BypassGovernanceRetention",
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": "sts:GetFederationToken",
            "Resource": "*",
            "Condition": {}
        }
    ]
}
