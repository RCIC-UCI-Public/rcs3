                    [ "AWS/S3/Storage-Lens", "StorageBytes", "metrics_version", "1.0", "storage_class", "STANDARD", "aws_region", "%REGION%", "aws_account_number", "%ACCOUNT%", "configuration_id", "%LENS%", "record_type", "ACCOUNT", { "id": "s_bytes", "region": "%REGION%", "color": "#bcbd22" } ],
                    [ ".", "DeleteMarkerStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_dm_bytes", "region": "%REGION%", "color": "#ffbb78" } ],
                    [ ".", "NonCurrentVersionStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_ncv_bytes", "region": "%REGION%", "color": "#dbdb8d" } ],
                    [ ".", "StorageBytes", ".", ".", ".", "GLACIER", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "g_bytes", "region": "%REGION%" } ],
                    [ ".", "DeleteMarkerStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "g_dm_bytes", "region": "%REGION%" } ],
                    [ ".", "NonCurrentVersionStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "g_ncv_bytes", "region": "%REGION%", "color": "#c7c700" } ],
                    [ ".", "StorageBytes", ".", ".", ".", "DEEP_ARCHIVE", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "d_bytes", "region": "%REGION%" } ],
                    [ ".", "DeleteMarkerStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "d_dm_bytes", "region": "%REGION%" } ],
                    [ ".", "NonCurrentVersionStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "d_ncv_bytes", "region": "%REGION%", "color": "#c7c7c7" } ]
                ],
                "view": "timeSeries",
                "stacked": false,
                "region": "%REGION%",
                "stat": "Sum",
                "period": 86400,
                "yAxis": {
                    "right": {
                        "label": "Cost $",
                        "showUnits": false
                    },
                    "left": {
                        "label": "Storage Bytes",
                        "showUnits": false
                    }
                },
                "title": "Storage Costs"
            }
        },
        {
            "height": 4,
            "width": 23,
            "y": 0,
            "x": 0,
            "type": "metric",
            "properties": {
