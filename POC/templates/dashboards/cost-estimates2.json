                    [ { "expression": "(1-EDISCOUNT)* (  ((sum_all_r - (sb_sumppl_r + g_sumppl_r + d_sumppl_r))*.0004/10.0) + (sb_sumppl_r*.005) + (g_sumppl_r*.03) + (d_sumppl_r*0.05))/1000", "label": "API Cost (mean: $ ${AVG})", "id": "costs_api", "yAxis": "right", "region": "%REGION%" , "visible" : false} ],
