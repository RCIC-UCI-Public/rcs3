        {
            "height": %HEIGHT%,
            "width": 23,
            "type": "metric",
            "properties": 
            {
                "sparkline": true,
                "metrics": [
                    [ { "expression": "SUM([e5,e8])", "label": "Total Storage", "id": "e2", "yAxis": "right", "region": "%REGION%" ,  "color": "#7111f0"} ],
                    [ "AWS/S3", "NumberOfObjects", "BucketName", "%OWNER%-%SYSTEM%-%BUCKET%", "StorageType", "AllStorageTypes", { "id": "num_objs", "region": "%REGION%", "label": "Object Count", "color": "#b7a5cf"  } ],
                    [ { "expression": "SUM([cost_stor,cost_api ])", "label": "Daily Storage+API (mean: $ ${AVG})", "id": "sum_cost", "yAxis": "right", "region": "%REGION%" , "color": "#36b000"} ],
                    [ { "expression": ".9095*g_bytes", "label": "Glacier TBytes", "id": "e5", "region": "%REGION%", "color": "#0273c9" } ],
                    [ { "expression": ".9095*s_bytes", "label": "Standard TBytes", "id": "e8", "region": "%REGION%" , "color": "#f76f20"} ],
                    [ { "expression": ".9095*g_snap_bytes", "label": "Glacier Snapshot TBytes", "id": "e6", "region": "%REGION%", "color": "#66b6f2" } ],
                    [ { "expression": ".9095*s_snap_bytes", "label": "Standard Snapshot TBytes", "id": "e7", "region": "%REGION%" ,  "color": "#ff9f67"} ],
                    [ "AWS/S3/Storage-Lens", "StorageBytes", "metrics_version", "1.0", "storage_class", "STANDARD", "aws_region", "%REGION%", "bucket_name", "%OWNER%-%SYSTEM%-%BUCKET%", "aws_account_number", "%ACCOUNT%", "configuration_id", "%LENS%", "record_type", "BUCKET", { "id": "s_bytes", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "SUM([s_dm_bytes,s_ncv_bytes])", "label": "STANDARD Snapshot Bytes", "id": "s_snap_bytes", "region": "%REGION%", "visible": false } ],
                    [ "AWS/S3/Storage-Lens", "StorageBytes", "metrics_version", "1.0", "storage_class", "GLACIER", "aws_region", "%REGION%", "bucket_name", "%OWNER%-%SYSTEM%-%BUCKET%", "aws_account_number", "%ACCOUNT%", "configuration_id", "%LENS%", "record_type", "BUCKET", { "id": "g_bytes", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "SUM([g_ncv_bytes,g_dm_bytes])", "label": "GLACIER - Snapshot Bytes", "id": "g_snap_bytes", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "(1-SDISCOUNT)*((s_gb_mo*S3STD) + (g_gb_mo*S3GLACIER))", "label": "Daily Storage Cost", "id": "cost_stor", "yAxis": "right", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "s_bytes*9.313e-10/days", "label": "GB-Months-Standard", "id": "s_gb_mo", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "g_bytes*9.313e-10/days", "label": "GB-Months-Glacier", "id": "g_gb_mo", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "IF(MONTH(g_bytes) == 2, 28, IF(MONTH(g_bytes)==4 || MONTH(g_bytes)==6 || MONTH(g_bytes)==9 || MONTH(g_bytes)==11,30,31))", "label": "Days in Month", "id": "days", "region": "%REGION%", "visible": false } ],
                    [ "AWS/S3/Storage-Lens", "DeleteMarkerStorageBytes", "metrics_version", "1.0", "storage_class", "STANDARD", "aws_region", "%REGION%", "bucket_name", "%OWNER%-%SYSTEM%-%BUCKET%", "aws_account_number", "%ACCOUNT%", "configuration_id", "%LENS%", "record_type", "BUCKET", { "id": "s_dm_bytes", "region": "%REGION%", "visible": false } ],
                    [ ".", "NonCurrentVersionStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_ncv_bytes", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "SUM([g_post_r,g_put_r,g_list_r])", "label": "Glacier-Put-Post-List-Requests (mean: ${AVG})", "id": "g_ppl_r", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "SUM([s_list_r,b_list_r,b_put_r,s_put_r,b_post_r,s_post_r])", "label": "Bucket-Standard-Put-Post-List-Requests (mean: ${AVG})", "id": "sb_ppl_r", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "SUM([g_all_r,b_all_r,s_all_r])", "label": "AllRequests (mean: ${AVG})", "id": "sum_al_r", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "(1-EDISCOUNT)* (  ((sum_al_r - (sb_ppl_r + g_ppl_r))*.0004/10.0) + (sb_ppl_r*.005) + (g_ppl_r*.03))/1000", "label": "API Cost (mean: $ ${AVG})", "id": "cost_api", "yAxis": "right", "region": "%REGION%", "visible": false } ],
                    [ { "expression": "RUNNING_SUM(cost_api)", "label": "SUM API Cost ($ ${MAX})", "id": "rsum_cost_api", "yAxis": "right", "region": "%REGION%", "visible": false } ],
                    [ "AWS/S3/Storage-Lens", "PostRequests", "metrics_version", "1.0", "storage_class", "GLACIER", "aws_region", "%REGION%", "bucket_name", "%OWNER%-%SYSTEM%-%BUCKET%", "aws_account_number", "%ACCOUNT%", "configuration_id", "%LENS%", "record_type", "BUCKET", { "label": "Glacier - PostRequests", "id": "g_post_r", "visible": false, "region": "%REGION%" } ],
                    [ ".", "PutRequests", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "label": "Glacier - PutRequests", "id": "g_put_r", "visible": false, "region": "%REGION%" } ],
                    [ ".", "ListRequests", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "label": "Glacier - ListRequests", "id": "g_list_r", "visible": false, "region": "%REGION%" } ],
                    [ "...", "STANDARD", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_list_r", "label": "Standard - ListRequests", "visible": false, "region": "%REGION%" } ],
                    [ "...", "-", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "b_list_r", "label": "Bucket - ListRequests", "visible": false, "region": "%REGION%" } ],
                    [ ".", "PutRequests", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "b_put_r", "label": "Bucket - PutRequests", "visible": false, "region": "%REGION%" } ],
                    [ "...", "STANDARD", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_put_r", "label": "Standard - PutRequests", "visible": false, "region": "%REGION%" } ],
                    [ ".", "PostRequests", ".", ".", ".", "-", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "b_post_r", "label": "Bucket - PostRequests", "visible": false, "region": "%REGION%" } ],
                    [ "...", "STANDARD", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_post_r", "label": "Standard - PostRequests", "visible": false, "region": "%REGION%" } ],
                    [ ".", "AllRequests", ".", ".", ".", "GLACIER", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "g_all_r", "label": "GLACIER - All Requests", "visible": false, "region": "%REGION%" } ],
                    [ "...", "-", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "b_all_r", "label": "Bucket - AllRequests", "visible": false, "region": "%REGION%" } ],
                    [ "...", "STANDARD", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "s_all_r", "label": "Standard - AllRequests", "visible": false, "region": "%REGION%" } ],
                    [ ".", "NonCurrentVersionStorageBytes", ".", ".", ".", "GLACIER", ".", ".", "aws_account_number", "%ACCOUNT%", "bucket_name", "%OWNER%-%SYSTEM%-%BUCKET%", ".", ".", "record_type", "BUCKET", { "id": "g_ncv_bytes", "visible": false } ],
                    [ ".", "DeleteMarkerStorageBytes", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", { "id": "g_dm_bytes", "visible": false } ]
                ],
                "view": "singleValue",
                "stacked": false,
                "region": "%REGION%",
                "stat": "Sum",
                "period": 86400,
                "yAxis": {
                    "right": {
                        "label": "Cost $",
                        "showUnits": true 
                    },
                    "left": {
                        "label": "Storage Bytes",
                        "showUnits": false
                    }
                },
                "title": "%OWNER% - %SYSTEM%"
            }
        }
