{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "createLogGroup"
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "arn:aws:logs:xxxregionxxx:xxxaccountidxxx:*"
        },
        {
            "Sid": "createAndWriteLogStream"
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:xxxregionxxx:xxxaccountidxxx:log-group:/aws/lambda/separatebucketandkeyfroms3uri:*"
            ]
        },
        {
            "Sid": "readS3Reports",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject"
            ],
            "Resource": [
                "arn:aws:s3:::rcs3-godfather-uci-s-bucket",
                "arn:aws:s3:::rcs3-godfather-uci-s-bucket/*"
            ]
        }
    ]
}