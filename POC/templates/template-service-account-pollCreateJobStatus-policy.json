{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "writeLogEvents"
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:xxxregionxxx:xxxaccountidxxx:log-group:/aws/lambda/pollCreateJobStatus:*"
            ]
        },
        {
            "Sid": "queryS3BatchJobs",
            "Effect": "Allow",
            "Action": [
                "s3:ListJobs",
                "s3:DescribeJob"
            ],
            "Resource": [
                "*"
            ]
        }
    ]
}
